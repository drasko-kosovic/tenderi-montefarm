            <column name="opis_postupka" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="vrsta_postupka" type="varchar(255)">
                <constraints nullable="false" />
            </column>
            <column name="datum_objave" type="date">
                <constraints nullable="true" />
